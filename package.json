{
    "name": "@uppercod/postcss-import",
    "version": "1.1.2",
    "description": "resolves the `@import` and creates the `tree` property in the result object, which defines the imports",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/postcss-import.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/postcss-import/issues"
    },
    "homepage": "https://github.com/UpperCod/postcss-import#readme",
    "dependencies": {
        "@uppercod/request": "^1.1.1",
        "postcss": "^7.0.32",
        "resolve-css": "^1.0.3"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^8.1.0",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "esm": "^3.2.25",
        "rollup": "^2.21.0",
        "tailwindcss": "^1.7.6"
    },
    "typings": "types.d.ts",
    "ava": {
        "require": [
            "esm"
        ],
        "files": [
            "test/**/*.test.js"
        ]
    },
    "exports": {
        "import": "./ems/index.js",
        "require": "./cjs/index.js"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
